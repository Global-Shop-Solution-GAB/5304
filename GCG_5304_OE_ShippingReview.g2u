	F.Intrinsic.Control.For(V.Local.iCnt,0,V.DataTable.dtAllShip.RowCount--,1)
		gui.ProPress.proBar.Value(v.Local.iCnt)
		f.Intrinsic.Math.Mod(v.Local.iCnt,2,v.Local.iRet)
		Gui.frmShip.GsGCAllShip.GetCellValueByColumnName("gvAllShip","SHP_HLD_FLAG",v.Local.iCnt,v.Local.hold)
		
		f.Intrinsic.Control.If(v.Local.iRet,=,0)
			Gui.frmShip.GsGCAllShip.SetRowAppearance("gvAllShip",V.Local.iCnt,"BackColor","AliceBlue")
		f.Intrinsic.Control.EndIf
		
		f.Intrinsic.Control.If(V.Local.Hold,=,True)
			Gui.frmShip.GsGCAllShip.SetRowAppearance("gvAllShip",V.Local.iCnt,"BackColor","Pink")
		f.Intrinsic.Control.EndIf
	F.Intrinsic.Control.Next(V.Local.iCnt)
	
	'Calculating MRP
	f.Intrinsic.UI.ChangeWaitStatus("Processing MRP Calculation...")
	f.Data.DataView.Create("dtAllShip","dvAllShipPartLoc")
	f.Data.DataView.ToDataTableDistinct("dtAllShip","dvAllShipPartLoc","PartLoc","Part*!*Location",True)
	f.Data.DataView.Close("dtAllShip","dvAllShipPartLoc")
	gui.ProPress.proBar.Max(v.DataTable.PartLoc.RowCount)
	f.Data.DataView.Create("dtAllShip","dvAllShipMRP")
	f.Intrinsic.Control.For(v.Local.iCnt,0,v.DataTable.PartLoc.RowCount--,1)
		gui.ProPress.proBar.Value(v.Local.iCnt)
		f.Intrinsic.String.IsInString(v.DataTable.PartLoc(v.Local.iCnt).Part!FieldValString,"AF-1430094",True,v.Local.bContain)
		f.Intrinsic.Control.If(v.Local.bContain)			
		f.Intrinsic.Control.EndIf
		
		v.Local.sPart.Set(v.DataTable.PartLoc(v.Local.iCnt).Part!FieldValString)
	 	f.Intrinsic.String.Build("Part = '{0}' and Location = '{1}'",v.Local.sPart.PSQLFriendly,v.DataTable.PartLoc(v.Local.iCnt).Location!FieldValString,v.Local.sFilter)				

	 	f.Data.DataView.SetFilter("dtAllShip","dvAllShipMRP",v.Local.sFilter)
	 	f.Data.DataView.SetSort("dtAllShip","dvAllShipMRP","DATE_ITEM_PROM ASC")
	 	v.Local.fOnhand.Set(v.DataView.dtAllShip!dvAllShipMRP(0).Qty_Onhand!FieldValString)
	 	f.Intrinsic.Control.For(v.Local.iInner,0,v.DataView.dtAllShip!dvAllShipMRP.RowCount--,1)
	 		f.Intrinsic.Control.If(v.DataView.dtAllShip!dvAllShipMRP(v.Local.iInner).Qty_BO!FieldValFloat,<=,v.Local.fOnhand)
	 			f.Intrinsic.Math.Sub(v.Local.fOnhand,v.DataView.dtAllShip!dvAllShipMRP(v.Local.iInner).Qty_BO!FieldValFloat,v.Local.fOnhand)
	 			v.Local.iIndex.Set(v.DataView.dtAllShip!dvAllShipMRP(v.Local.iInner).Num!FieldValString)
	 			Gui.frmShip.GsGCAllShip.setRowAppearance("gvAllShip",v.Local.iIndex,"BackColor","LightGreen")
	 		f.Intrinsic.Control.EndIf
	 	f.Intrinsic.Control.Next(v.Local.iInner)	 	
	f.Intrinsic.Control.Next(v.Local.iCnt)
	f.Data.DataView.Close("dtAllShip","dvAllShipMRP")
	f.Data.DataTable.Close("PartLoc")	
	gui.ProPress..Visible(False)
F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.End 
