Program.Sub.loadDTAllShip.Start
f.Intrinsic.Control.Try
	v.Local.sError.Declare
	V.Local.iCnt.Declare(Long)
	V.Local.iRow.Declare(Long)
	V.Local.sFilter.Declare(String)
	V.Local.sRow.Declare(String)
	v.Local.sOrderRecs.Declare
	v.Local.iTime.Declare
	Gui.frmShip.lbl1.Caption("Loading Shipments....")
	
	F.Intrinsic.Control.If(V.DataTable.dtAllShip.Exists,=,True)
		F.Data.DataTable.Close("dtAllShip")
	F.Intrinsic.Control.EndIf

	V.Local.sSQL.Declare(String,"Select Distinct CASE WHEN VOL.PICK_LIST_PRINTED = 'Y' THEN 'RePrint' ELSE 'Print' END AS PickLs,VOL.Customer,'' as NAME_CUSTOMER, '' as CUSTOMER_PO,'' as SalesPerson,VOL.PRODUCT_LINE,VOL.LOCATION,VOL.PART,VOL.PART + VOL.LOCATION as PartLoc,'Link' as Link,'' as HasLink,VOL.DESCRIPTION,VOL.ORDER_NO,LEFT(VOL.Record_NO,3) AS RECORD_NO,VOL.ORDER_NO + '-' + VOL.Record_NO AS OrderRec,VOL.ORDER_NO + '-' + LEFT(VOL.Record_NO,3) AS OrderRecNotes,'' as WO,CASE WHEN VOL.FLAG_USE_MQD != 'Y' THEN VOL.QTY_BO WHEN FLAG_USE_MQD = 'Y' THEN VOSS.QTY_OPEN END AS QTY_BO,VOL.WEIGHT,CASE WHEN VOL.FLAG_USE_MQD != 'Y' THEN VOL.DATE_ITEM_PROM WHEN FLAG_USE_MQD = 'Y' THEN IF(VOSS.DATE_DUE = '1900-01-01',CONVERT('9999-12-31',SQL_DATE),VOSS.DATE_DUE) END AS  DATE_ITEM_PROM,VOL.SHIP_ID,VOL.ORDER_NO + '-' + VOL.SHIP_ID as OrderRecShipID,'' as CITY_SHIP,'' as SHIP_VIA_SHIP,'' as CARRIER_CD,CONVERT(VOL.STAGED,SQL_BIT) AS STAGED,CASE WHEN FLAG_USE_MQD = 'Y' THEN 'Ship Schd' ELSE '-' END AS FLAG_USE_MQD,CONVERT(0,SQL_BIT) AS SHP_HLD_FLAG,VOL.QTY_SHIPPED,'' as NOTES,rtrim(VOL.USER_1) as USER_1,rtrim(VOL.USER_2) as USER_2,rtrim(VOL.USER_3) as USER_3,rtrim(VOL.USER_4) as USER_4,rtrim(VOL.USER_5) as USER_5 From V_ORDER_LINES VOL LEFT JOIN V_OE_SHIP_SCHED VOSS ON VOSS.ORDER_NO = VOL.ORDER_NO AND VOSS.ORDER_LINE = VOL.RECORD_NO WHERE QTY_BO <> 0 and ISNULL(VOSS.FLAG_CLOSED,'') <> 'Y' ORDER BY DATE_ITEM_PROM;")	

	F.Data.DataTable.CreateFromSQL("dtAllShip","con",V.Local.sSQL,True)
	f.Data.DataTable.AddExpressionColumn("dtAllShip","PartLocRTrim","String","Trim(PartLoc)")
	f.Data.DataTable.AddColumn("dtAllShip","Qty_Onhand","float",0)
	
	'***BN, Dec 12, 2018 Use dictionaries to populate columns just to discharge the load of sql engine, make the script run faster (reduced the time from 80 - 100 secs to 27 - 30 secs to load)***
	'F.Intrinsic.Debug.TimerStart("tDict")	
	'Notes from GAB_4167_SO_NOTES
	f.Data.Dictionary.CreateFromSQL("NotesDict","con","Select SO + '-' + SOLine as OrderRecNotes, ISNULL(NOTES, '') as NOTES from GAB_4167_SO_NOTES")
	f.Data.Dictionary.SetDefaultReturn("NotesDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","NotesDict","OrderRecNotes","Notes")
	f.Data.Dictionary.Close("NotesDict")
	
	'Customer PO Dictionary, SHP_HLD_FLAG and SalesPerson dictionaries from OrderHeader
	f.Data.DataTable.CreateFromSQL("OrderHeader","con","Select Order_No,rtrim(CUSTOMER_PO) as Customer_PO,SalesPerson,CASE WHEN SHP_HLD_FLAG = 02 OR SHP_HLD_FLAG = 04 THEN CONVERT(1,SQL_BIT) WHEN SHP_HLD_FLAG = NULL THEN CONVERT(0,SQL_BIT) ELSE CONVERT(0,SQL_BIT) END as SHP_HLD_FLAG from Order_Header where RECORD_NO = '  A'",True)
	'Customer PO dictionary from OrderHeader
	f.Data.Dictionary.CreateFromDatatable("CustPODict","OrderHeader","Order_No","Customer_PO")		
	f.Data.Dictionary.SetDefaultReturn("CustPODict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","CustPODict","Order_No","Customer_PO")
	f.Data.Dictionary.Close("CustPODict")
	'SHP_HLD_FLAG dictionary from OrderHeader
	f.Data.Dictionary.CreateFromDatatable("SHPFlagDict","OrderHeader","Order_No","SHP_HLD_FLAG")		
	f.Data.Dictionary.SetDefaultReturn("SHPFlagDict",False)
	f.Data.DataTable.FillFromDictionary("dtAllShip","SHPFlagDict","Order_No","SHP_HLD_FLAG")
	f.Data.Dictionary.Close("SHPFlagDict")
	'SalesPerson dictionary from OrderHeader
	f.Data.Dictionary.CreateFromDatatable("SalesPersonDict","OrderHeader","Order_No","SalesPerson")		
	f.Data.Dictionary.SetDefaultReturn("SalesPersonDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","SalesPersonDict","Order_No","SalesPerson")
	f.Data.Dictionary.Close("SalesPersonDict")
	
	f.Data.DataTable.Close("OrderHeader")
	
	'QtyOnhand dictionary from inventory master
	f.Data.Dictionary.CreateFromSQL("QtyOnhandDict","con","Select Part + Location as PartLoc,Qty_Onhand from Inventory_Mstr")
	f.Data.Dictionary.SetDefaultReturn("QtyOnhandDict",0)
	f.Data.DataTable.FillFromDictionary("dtAllShip","QtyOnhandDict","PartLoc","Qty_Onhand")
	f.Data.Dictionary.Close("QtyOnhandDict")	
	
	'Customer Name from Customer Master dict
	f.Data.Dictionary.CreateFromSQL("CustNameDict","con","Select CUSTOMER,NAME_CUSTOMER from V_CUSTOMER_MASTER")
	f.Data.Dictionary.SetDefaultReturn("CustNameDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","CustNameDict","CUSTOMER","NAME_CUSTOMER")
	f.Data.Dictionary.Close("CustNameDict")
	
	'VOS.CITY_SHIP,VOS.SHIP_VIA_SHIP,VOS.CARRIER_CD Dictionaries
	f.Data.DataTable.CreateFromSQL("OrderShipTo","con","Select ORDER_NO + '-' + SHIP_ID as OrderRecShipID,CITY_SHIP,SHIP_VIA_SHIP,CARRIER_CD from V_ORDER_SHIP_TO",True)
	'CITY_SHIP
	f.Data.Dictionary.CreateFromDatatable("CityShipDict","OrderShipTo","OrderRecShipID","CITY_SHIP")		
	f.Data.Dictionary.SetDefaultReturn("CityShipDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","CityShipDict","OrderRecShipID","CITY_SHIP")
	f.Data.Dictionary.Close("CityShipDict")
	'SHIP_VIA_SHIP
	f.Data.Dictionary.CreateFromDatatable("ShipViaDict","OrderShipTo","OrderRecShipID","SHIP_VIA_SHIP")		
	f.Data.Dictionary.SetDefaultReturn("ShipViaDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","ShipViaDict","OrderRecShipID","SHIP_VIA_SHIP")
	f.Data.Dictionary.Close("ShipViaDict")	
	'CARRIER_CD
	f.Data.Dictionary.CreateFromDatatable("CarrierDict","OrderShipTo","OrderRecShipID","CARRIER_CD")		
	f.Data.Dictionary.SetDefaultReturn("CarrierDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","CarrierDict","OrderRecShipID","CARRIER_CD")
	f.Data.Dictionary.Close("CarrierDict")	
	
	f.Data.DataTable.Close("OrderShipTo")
	
	'Get the open WOs linked to the Sales Order lines
	'Create Table dtOrdertoWO2
	f.Data.DataView.Create("dtAllShip","dvOrderRec")
	f.Data.DataView.ToString("dtAllShip","dvOrderRec","OrderRec","","','",v.Local.sOrderRecs)
	f.Data.DataView.Close("dtAllShip","dvOrderRec")		
	f.Data.DataTable.CreateFromSQL("OrdtoWO","con","Select Order_No + '-' + Order_Line as OrderRec,IF(SUFFIX = '',JOB,JOB + '-' + SUFFIX) as WO  from Order_To_WO",True)
	f.Data.DataTable.CreateFromSQL("OrdHeader","con","select IF(SUFFIX = '',JOB,JOB + '-' + SUFFIX) as WO,Date_Closed from V_Job_header",True)
	
	F.Data.Linq.Join("LeftJoin","DataTable","OrdtoWO*!*OtoW","DataTable","OrdHeader*!*oh","OtoW.WO = oh.WO","OtoW.OrderRec*!*OtoW.WO*!*oh.Date_Closed","","","","dtOrdertoWO",True)
	f.Data.DataView.Create("dtOrdertoWO","dvOrdertoWO",22,"Date_Closed = #1900/01/01#","")
	f.Data.DataView.ToDataTable("dtOrdertoWO","dvOrdertoWO","dtOrdertoWO2")	
	'Create dictionary from dtOrdertoWO2
	f.Data.Dictionary.CreateFromDatatable("WODict","dtOrdertoWO2","OrderRec","WO")	
	f.Data.Dictionary.SetDefaultReturn("WODict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","WODict","OrderRec","WO")
	f.Data.Dictionary.Close("WODict")	
	
	'HasLink
	f.Data.Dictionary.CreateFromSQL("HasLinkDict","con","select rtrim(ID),'Yes' as HasLink from LINK_DATA where type = '00030'")	
	f.Data.Dictionary.SetDefaultReturn("HasLinkDict","")
	f.Data.DataTable.FillFromDictionary("dtAllShip","HasLinkDict","PartLocRTrim","HasLink")
	f.Data.Dictionary.Close("HasLinkDict")
	
	'F.Intrinsic.Debug.TimerElapsed("tDict",V.Local.iTime)
	f.Intrinsic.Variable.AddRV("iTime",v.Local.iTime)
	
	f.Data.DataTable.Close("OrdtoWO")
	f.Data.DataTable.Close("OrdHeader")
	f.Data.DataTable.Close("dtOrdertoWO")
	f.Data.DataTable.Close("dtOrdertoWO2")
	
	'Set Series
	f.Data.DataTable.AddColumn("dtAllShip","Num","Long")
	f.Data.DataTable.SetSeries("dtAllShip","Num",0,1)	
	
	F.Intrinsic.Control.CallSub(loadDVGVAllShip)

	
F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}GAB Version: {5}}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	f.Intrinsic.Control.CallSub("Unload")
F.Intrinsic.Control.EndTry	
Program.Sub.loadDTAllShip.End

